<!--pages/login/login.wxml-->
<view class="container">
  <view class="login_space"></view>
  <view>
    <image src="../../img/FirstAid LOGO.png" mode="aspectFit" class="main_logo">
    </image>
  </view>
  <view class="loginbox">
    <view class="register_complete">已完成注册，请登录!</view>
    <form action="">
      <view class="login_form">
        <view class="login_form_cell">
          <input class="login_input" name="input" type="text" placeholder="账号"
            placeholder-style=" text-align: center ;color: #ffffff" />
        </view>
        <view class="login_form_cell">
          <input class="login_input" password="true" name="input" placeholder="密码"
            placeholder-style=" text-align: center;color: #ffffff" />
        </view>
        <view class="button-sp-area">
          <button class="but-login login" hover-class="button-hover">
            <view class="but_text">登录 </view>
          </button>
        </view>
        <view class="loginToRegistP">还没有账号?<navigator class="loginToRegistA">立即注册</navigator>
        </view>
        <view>
          <view class="divLineL"></view>
          <view class="divLineT">OR</view>
          <view class="divLineR"></view>
        </view>
        <view class="button-sp-area">
          <button class="but-login wxlogin" hover-class="button-hover">
            <view class="but_text">使用微信账号登录 </view>
          </button>
        </view>
      </view>
    </form>
  </view>
  <view class="login_space"></view>
</view>